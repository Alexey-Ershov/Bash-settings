	email = alexxey.ershov@gmail.com
	name = Alexey-Ershov
[core]
	autocrlf = input
	safecrlf = true
	quotepath = off

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
	default = simple
